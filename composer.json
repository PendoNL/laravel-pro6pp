{
  "name": "pendonl/laravel-pro6pp",
  "description": "Laravel interface to Pro6pp API PHP Wrapper",
  "authors": [
    {
      "name": "Joshua de Gier",
      "email": "info@pendo.nl"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "pendonl/pro6pp-php-wrapper": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "PendoNL\\LaravelPro6pp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PendoNL\\LaravelPro6pp\\Test\\": "tests"
    }
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "PendoNL\\LaravelPro6pp\\LaravelPro6ppServiceProvider"
      ],
      "aliases": {
        "Pro6pp": "PendoNL\\LaravelPro6pp\\Facade"
      }
    }
  }
}
